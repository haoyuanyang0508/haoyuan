        // 遊戲進行時的當前場景索引
        let currentScene = 0;
        // HOME 鍵元素
        const homeBtn = document.getElementById('homeBtn');
        // 提示按鈕元素
        const hintBtn = document.getElementById('hintBtn');
        // 在遊戲進行時的當前場景索引下方新增一個全局變數，用於追蹤提示使用次數
        let hintCount = 3;
        // 在遊戲進行時的當前場景索引下方定義一個全局變數，用於保存計時器ID。
        let timerId;
        // 倒數計時器的秒數
        let countdownSeconds = 10;
        // 設置計時器顯示的元素
        const timerElement = document.getElementById('timer');
        // 按鈕隱藏
        // let index = 0;

        const sceneToSoundId = {
            1: 's1',
            7: 's7',
            10: 's10',
            17: 's17',
            20: 's20',
            25: 's25',
            32: 's32'
        };

        // 啟動計時器
        function startTimer() {
            const gameContent = document.querySelector('.game-content');
            if (gameContent.style.display === "block") {
                timerId = setInterval(updateTimer, 1000);
            }
        }
        // 更新計時器
        function updateTimer() {
            countdownSeconds--;
            if (countdownSeconds >= 0) {
                timerElement.textContent = `剩餘時間：${countdownSeconds}秒`;
                if (hintCount > 0) {
                    hintBtn.textContent = `提示：${hintCount}次`;
                }
                // 根據倒數計時狀態添加/移除CSS類
                if (countdownSeconds <= 3) {
                    document.body.classList.add('countdown-filter');
                } else {
                    document.body.classList.remove('countdown-filter');
                }
            } else {
                clearInterval(timerId);
                handleTimeout();
                hintBtn.style.display = "none";
            }
        }
        // 處理倒數計時結束的情況
        function handleTimeout() {
            const scene = scenes[currentScene];
            if (scene.hasOptions) {
                // 如果是具有選項的場景，將遊戲導向場景46
                currentScene = 46;
            } else {
                // 如果是沒有選項的場景，將遊戲導向下一個場景（場景7的下一個場景）
                currentScene = scene.nextScene;
            }
            showScene();
        }
        // 重置計時器
        function resetTimer() {
            clearInterval(timerId);
            countdownSeconds = 10; // 重置倒數計時秒數為10秒
            timerElement.textContent = `剩餘時間：${countdownSeconds}秒`;
        }
