        //         timerElement.style.display = "none";
        //         hintBtn.style.display = "none";
        //         clearInterval(timerId);
        //         // 遊戲結束以及成功拯救妻子的頁面不顯示下一頁按鈕
        //         if (currentScene !== 13 && currentScene !== 27 && currentScene !== 31 && currentScene !== 34 && currentScene !== 39 && currentScene !== 45 && currentScene !== 46) {
        //             nextBtn.style.display = "block";
        //         } else {
        //             nextBtn.style.display = "none";
        //         }
        //     }
        //     // 設定所有具有選項的場景的hasTimer屬性
        //     scenes.forEach((scene) => {
        //         if (scene.hasOptions) {
        //             scene.hasTimer = true;
        //         } else {
        //             scene.hasTimer = false;
        //         }
        //     });
        //     // 播放當前場景的音效
        //     if (currentScene === 1) {
        //         plays1();
        //     } else if (currentScene === 7) {
        //         plays7();
        //     } else if (currentScene === 10) {
        //         plays10();
        //     } else if (currentScene === 17) {
        //         plays17();
        //     } else if (currentScene === 20) {
        //         plays20();
        //     } else if (currentScene === 25) {
        //         plays25();
        //     } else if (currentScene === 32) {
        //         plays32();
        //     } 
        // }
        let previousSoundElement = null;

        function showScene() {
            const scene = scenes[currentScene];
            const storyElement = document.getElementById('story');
            const text = storyElement.textContent = scene.story;
            storyElement.textContent = '';
            let speed = 100;
            let index = 0;
            const bodyElement = document.body;
            bodyElement.style.backgroundImage = `url('${backgroundURLs[currentScene]}')`;

            function textInput() {
                if (index < text.length) {
                    storyElement.append(text.charAt(index));
                    index++;
                    setTimeout(textInput, speed);
                } else {
                    afterTextComplete();
                }
            }

            const sea = document.getElementById('sea');

            if (previousSoundElement) {
                previousSoundElement.pause();
                previousSoundElement.currentTime = 0; // 重置音效到開始位置
            }

            const soundId = sceneToSoundId[currentScene];
            const soundElement = document.getElementById(soundId);

            if (soundId) {
                soundElement.play();
                previousSoundElement = soundElement;  // 更新 previousSoundElement 的值
            } else {
                sea.play();
                previousSoundElement = sea;  // 如果沒有對應的音效則播放 sea 音樂並更新 previousSoundElement 的值
            }


            // if(scene.audioId) {
            //     const previousAudio = document.getElementById(scene.audioId);
            //     if(previousAudio) {
            //         previousAudio.pause();
            //         previousAudio.currentTime = 0;
            //     }
            // }

            // // ... 其他 showScene 的程式碼 ...

            // // 播放當前場景的音樂
            // let currentAudio;
            // if(scene.audioId) {
            //     currentAudio = document.getElementById(scene.audioId);
            // } else {
            //     currentAudio = document.getElementById('sea');
            // }

            // if(currentAudio) {
            //     currentAudio.play();
            // }


            const optionBtn1 = document.getElementById('optionBtn1');
            const optionBtn2 = document.getElementById('optionBtn2');
            const nextBtn = document.getElementById('nextBtn');
            console.log("reset計時器後");
            console.log(text.length);
            console.log(scene.hasOptions);
            nextBtn.style.display = "none";
            optionBtn1.style.display = "none";
            optionBtn2.style.display = "none";
            timerElement.style.display = "none";
            hintBtn.style.display = "none";
            console.log("重製Btn");
            console.log(currentScene);

            function afterTextComplete() {
                console.log("所有文字都已輸出完畢！");

                // 重置計時器
                resetTimer();


                if (scene.hasOptions) {
                    optionBtn1.textContent = scene.option1Text;
                    optionBtn2.textContent = scene.option2Text;
                    optionBtn1.style.display = "block";
                    optionBtn2.style.display = "block";
                    nextBtn.style.display = "none";
                    timerElement.style.display = "block";
                    hintBtn.style.display = "block";
                    countdownSeconds = scene.countdownSeconds;
                    countdownSeconds = 10
                    timerElement.textContent = `剩餘時間：${countdownSeconds}秒`;
                    // 啟動計時器
                    clearInterval(timerId);
                    timerId = setInterval(updateTimer, 1000);
                    console.log("判斷打字是否結束");
                } else {
                    optionBtn1.style.display = "none";
                    optionBtn2.style.display = "none";
                    timerElement.style.display = "none";
                    hintBtn.style.display = "none";
                    nextBtn.style.display = "block";
                    clearInterval(timerId);
                    // 遊戲結束以及成功拯救妻子的頁面不顯示下一頁按鈕
                }
                if (currentScene !== 13 && currentScene !== 27 && currentScene !== 31 && currentScene !== 34 && currentScene !== 39 && currentScene !== 45 && currentScene !== 46) {
                    // nextBtn.style.display = "block";
                    console.log("判斷頁數");
                } else {
                    nextBtn.style.display = "none";
                    alert("遊戲結束!!");

                }
                // console.log( nextBtn);



                // if (scene.hasOptions) {
                //     optionBtn1.textContent = scene.option1Text;
                //     optionBtn2.textContent = scene.option2Text;
                //     optionBtn1.style.display = "block";
                //     optionBtn2.style.display = "block";
                //     nextBtn.style.display = "none";
                //     timerElement.style.display = "block";
                //     hintBtn.style.display = "block";
                //     countdownSeconds = scene.countdownSeconds;
                //     countdownSeconds = 10
                //     timerElement.textContent = `剩餘時間：${countdownSeconds}秒`;
                //     // 啟動計時器
                //     clearInterval(timerId);
                //     timerId = setInterval(updateTimer, 1000);
                //     console.log("判斷打字是否結束");
                // } else {
                //     optionBtn1.style.display = "none";
                //     optionBtn2.style.display = "none";
                //     timerElement.style.display = "none";
                //     hintBtn.style.display = "none";
                //     clearInterval(timerId);
                // // 遊戲結束以及成功拯救妻子的頁面不顯示下一頁按鈕
                // }
                // if (currentScene !== 13 && currentScene !== 27 && currentScene !== 31 && currentScene !== 34 && currentScene !== 39 && currentScene !== 45 && currentScene !== 46) {
                //     nextBtn.style.display = "block";
                //     console.log("判斷頁數");
                // } else {
                //     nextBtn.style.display = "none";
                // }
                // console.log( nextBtn);
            }


            textInput();
            console.log("textInput：" + index);

            // 隱藏或顯示選項按鈕

            // 設定所有具有選項的場景的hasTimer屬性
            scenes.forEach((scene) => {
                if (scene.hasOptions) {
                    scene.hasTimer = true;
                } else {
                    scene.hasTimer = false;
                }
            });

            // const soundId = sceneToSoundId[currentScene];
            // if (soundId) {
            //     const soundElement = document.getElementById(soundId);
            //     soundElement.play();
            // } else {
            //     const sea = document.getElementById('sea');
            //     sea.play();
            // }

        }



        // playsea();

        // 開始遊戲按鈕的點擊事件
        document.getElementById('startBtn').addEventListener('click', () => {
            // 播放背景音樂bgm
            sea.play();
            const startScreen = document.querySelector('.start-screen');
            const gameContent = document.querySelector('.game-content');
            const h1Element = document.querySelector('h1');
            h1Element.style.display = 'block'; // 顯示 h1 元素
            startScreen.style.display = "none";
            gameContent.style.display = "block";
            hintBtn.style.display = "block"; // 顯示提示按鈕
            currentScene = 0; // 重置為場景0
            showScene();
            // 啟動計時器
            startTimer();
            // 顯示 HOME 鍵
            const homeBtn = document.getElementById('homeBtn');
            homeBtn.style.display = "block";
        });
        // 下一頁按鈕的點擊事件
        document.getElementById('nextBtn').addEventListener('click', () => {
            // stopPreviousSceneAudio();
            clearInterval(timerId);
            // 檢查是否超時
            if (countdownSeconds <= 0) {
                handleTimeout();
            } else {
                const nextSceneIndex = scenes[currentScene].nextScene;
                currentScene = nextSceneIndex;
                showScene();
            }
        });
        // 停止前一个场景的音频
        // function stopPreviousSceneAudio() {
        //     const previousScene = scenes[currentScene];
        //     console.log(previousScene.audioId);
        //     if (previousScene.audioId) {
        //         const previousAudio = document.getElementById(previousScene.audioId);
        //         previousAudio.pause();
        //         previousAudio.currentTime = 0;
        //     }
        // }
        // 選項按鈕的點擊事件
        document.getElementById('optionBtn1').addEventListener('click', () => {
            // stopPreviousSceneAudio();
            const nextSceneIndex = scenes[currentScene].option1NextScene;
            currentScene = nextSceneIndex;
            showScene();
        });
        document.getElementById('optionBtn2').addEventListener('click', () => {
            // stopPreviousSceneAudio();
            const nextSceneIndex = scenes[currentScene].option2NextScene;
            currentScene = nextSceneIndex;
            showScene();
        });
        // HOME 鍵點擊事件處理
        homeBtn.addEventListener('click', () => {
            clearInterval(timerId);
            // 顯示提示框
            const confirmReturn = confirm("是否結束遊戲回到開始畫面？");
            if (confirmReturn) {
                // 停止前一個場景的音效
                // stopPreviousSceneAudio();
                // 重置背景圖像和計時器
                const bodyElement = document.body;
                bodyElement.style.backgroundImage = "url('https://media.discordapp.net/attachments/1086185318895603744/1135879685377630208/nancymuyi_A_beautiful_animated_picture_of_a_beam_of_sunlight_sh_b90b0592-0400-44e7-abb1-16fc84d4b9dc.png?width=1763&height=988')"; // 設置開始畫面的背景圖像
                const gameContent = document.querySelector('.game-content');
                const startScreen = document.querySelector('.start-screen');
                gameContent.style.display = "none";
                startScreen.style.display = "block";
                hintCount = 3; // 重新計算提示次數
                updateHintCounter(); // 更新提示按鈕文字
                resetTimer();
            } else {
                // 停止前一個場景的音效
                // stopPreviousSceneAudio();
                startTimer();
            }
        });
        // 提示按鈕點擊事件處理
        hintBtn.addEventListener('click', () => {
            const currentSceneData = scenes[currentScene];
            // 檢查是否有剩餘提示次數且場景有提示
            if (hintCount > 0 && currentSceneData.hasHint) {
                const currentHint = currentSceneData.hint;
                // 顯示提示框
                alert(currentHint);
                hintCount--; // 提示次數 -1
                // 更新提示按鈕文字，顯示剩餘次數
                hintBtn.textContent = `提示：${hintCount}次`;
                resetTimer();
            } else {
                if (hintCount === 0) {
                    // 提示次數已用完，顯示訊息
                    alert("你已經用完了所有的提示次數。");
                }
            }
            // 更新提示次數顯示
            updateHintCounter();
        });

        // 更新提示次數顯示
        function updateHintCounter() {
            const hintCounterElement = document.getElementById('hintCounter');
            hintCounterElement.textContent = hintCounter;
        }
        const sea = document.getElementById('sea');
        sea.play();

    </script>
</body>

</html>
